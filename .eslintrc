{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 11
    },
    "plugins": [
      "es"
    ],
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "camelcase": 0,
      "class-methods-use-this": 0,
      "consistent-return": 0,
      "eqeqeq": 1,
      "func-names": 0,
      "import/extensions": [
        "error",
        "always"
      ],
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "max-len": [
        "error",
        {
          "code": 9999
        }
      ],
      "new-cap": 0,
      "no-async-promise-executor": 0,
      "no-console": 0,
      "no-continue": 0,
      "no-param-reassign": 1,
      "no-plusplus": 0,
      "no-restricted-syntax": 0,
      "no-return-await": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": 1,
      "no-useless-escape": 0,
      "no-var": 1,
      "object-shorthand": [
        "error",
        "properties"
      ],
      "operator-linebreak": [
        "error",
        "after"
      ],
      "padded-blocks": 1,
      "prefer-arrow-callback": 0,
      "prefer-destructuring": 1,
      "prefer-object-spread": 0,
      "quotes": [
        "error",
        "single"
      ],
      "radix": 0,
      "space-before-function-paren": 0
    }
  }
